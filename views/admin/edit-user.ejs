<div class="admin_wrapper">
    <div class="left-sidebar">
        <ul>
            <li><a href="/topics">Manage Topics</a></li>
            <li><a href="/dashboard">Manage Posts</a></li>
            <li><a href="/my-shops">Manage Shop</a></li>
            <li><a href="#">Manage Tutorials</a></li>
            <li><a href="/team">Manage Team Members</a></li>
            <li><a href="/faq">Manage Faqs</a></li>
        </ul>
    </div>
    <div class="admin-content">
        <div class="button-group">
            <a href="/add-user" class="btn admin-btn">Add New users</a>
            <a href="/my-users" class="btn admin-btn">Manage users</a>
        </div>

        <div class="content">
            <h2 class="page-title">Edit user</h2>

            <div>
                <form action="/delete-user/<%= user._id %>?_method=DELETE" method="post">
                    <button type="submit" class="btn admin-btn">Delete this user</button>
                </form>
            </div>

            <% if(messages.error){ %>
                <ul class="error">
                    <li><%= messages.error %></li>
                </ul>
            <% } %>
           
            <form action="/edit-user/<%= user._id %>" method="post" enctype="multipart/form-data" class="add-post">
                <div>
                    <label for="username">Sorry, You are not authorised to edit user credentials</label>
                    
                </div>
               
            </form>

        </div>
    </div>
</div>
